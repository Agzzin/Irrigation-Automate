generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usuario {
  id       Int     @id @default(autoincrement())
  nome     String  @db.VarChar(100)
  email    String  @unique @db.VarChar(100)
  telefone String? @db.VarChar(20)
  senha    String  @db.VarChar(255)
}
